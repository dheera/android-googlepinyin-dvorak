            <softkey id="@id/softkey_ellipsis" param_data="â€¦" />
            <softkey id="@id/softkey_wave" param_data="~" />
            <softkey id="@id/softkey_at" param_data="\@" />
            <softkey id="@id/softkey_quote" param_data="&quot;" />
            <softkey id="@id/softkey_percent" param_data="%" />
            <softkey id="@id/softkey_colon" param_data=":" />
            <softkey id="@id/softkey_slash" param_data="/" />
            <softkey id="@id/softkey_semicolon" param_data=";" />
        </softkey_list>
    </softkeys>
</framework>