<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_9key_zh_shift" layout="@layout/softkey_icon_center_icon_bottom" multi_touch="true" content_description="@string/shift_key_content_desc">
            <action type="PRESS" keycode="LOCK_SHIFT" />
            <icon location="@id/icon" value="@attr/IconShiftOff" />
            <icon location="@id/icon_bottom" value="@attr/Icon9KeyShiftUnderline" />
        </softkey>
        <softkey id="@id/softkey_9key_zh_shift_locked" layout="@layout/softkey_icon_center_icon_bottom" multi_touch="true" content_description="@string/shift_shifted_key_content_desc">
            <action type="PRESS" keycode="UNLOCK_SHIFT" />
            <icon location="@id/icon" value="@attr/IconShiftOn" />
            <icon location="@id/icon_bottom" value="@attr/Icon9KeyShiftUnderlineHighlight" />
        </softkey>
    </softkeys>
</framework>