            <icon location="@id/icon" value="@attr/IconHeaderTabEnDeactive" />
        </softkey>
        <softkey id="@id/softkey_en_deactive_in_dashboard" layout="@layout/softkey_header_tab_with_separator" content_description="@string/english_dashboard_key_content_desc" alpha="@attr/IconTabDeactive">
