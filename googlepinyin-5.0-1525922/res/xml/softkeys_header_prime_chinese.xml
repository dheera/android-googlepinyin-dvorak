            <icon location="@id/icon" value="@attr/IconHeaderTabEnActive" />
        </softkey>
        <softkey id="@id/softkey_en_active_in_dashboard" layout="@layout/softkey_header_tab_active" content_description="@string/english_keyboard_key_content_desc">
