<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@+id/softkey_template_symbol" layout="@layout/softkey_label_medium" slide_sensitivity="NORMAL" multi_touch="true" content_description="$desc$">
            <action type="PRESS" keycode="PLAIN_TEXT" data="$param_data$" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_symbol">
            <softkey param_data="(" />
            <softkey param_data=")" />
            <softkey param_data="+" />
            <softkey param_data="-" />
            <softkey param_data="*" />
            <softkey param_data="/" />
            <softkey param_data="=" />
            <softkey param_data="~" />
            <softkey param_data=":" />
            <softkey param_data=";" />
            <softkey param_data="&quot;" />
            <softkey param_data="'" />
            <softkey param_data="!" />
            <softkey param_data="\?" />
            <softkey param_data="×" />
            <softkey param_data="÷" />
            <softkey param_data="&amp;" />
            <softkey param_data="{" />
            <softkey param_data="∑" />
            <softkey param_data="≠" />
            <softkey param_data="∞" />
        </softkey_list>
    </softkeys>
</framework>