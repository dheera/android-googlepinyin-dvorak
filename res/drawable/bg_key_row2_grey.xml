<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/softkey_bg_corner_radius" />
            <solid android:color="#99000000" />
        </shape>
    </item>
    <item android:left="1.0px" android:top="0.0px" android:right="1.0px" android:bottom="2.0px">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/softkey_bg_corner_radius" />
            <solid android:color="#ff7c7c7c" />
        </shape>
    </item>
    <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="2.0px">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/softkey_bg_corner_radius" />
            <solid android:color="#ff646464" />
        </shape>
    </item>
    <item android:left="1.0px" android:top="2.0px" android:right="1.0px" android:bottom="1.0px">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/softkey_bg_corner_radius" />
            <solid android:color="#99000000" />
        </shape>
    </item>
    <item android:left="1.0px" android:top="2.0px" android:right="1.0px" android:bottom="2.0px">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/softkey_bg_corner_radius" />
            <gradient android:startColor="#ff565656" android:endColor="#ff525252" android:angle="270.0" />
        </shape>
    </item>
</layer-list>