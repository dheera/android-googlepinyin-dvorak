<?xml version="1.0" encoding="utf-8"?>
<framework>
    <keyboard id="@id/keyboard_more_access_points_panel" class=".keyboard.Keyboard">
        <view id="@id/popup_keyboard_view" layout="@layout/popup_keyboard_access_points_panel">
            <motion_event_handler class=".libs.framework.keyboard.handler.BasicMotionEventHandler" />
            <softkeys>
                <softkey id="@id/softkey_access_points_show_more_apps" layout="@layout/softkey_access_points_show_more_apps_label">
                    <action type="PRESS" keycode="SHOW_MORE_APPS" />
                    <label location="@id/label" value="@string/label_more_apps" />
                </softkey>
            </softkeys>
            <key_mapping>
                <mapping view_id="@id/key_pos_show_more_apps" key_id="@id/softkey_access_points_show_more_apps" />
            </key_mapping>
        </view>
    </keyboard>
</framework>