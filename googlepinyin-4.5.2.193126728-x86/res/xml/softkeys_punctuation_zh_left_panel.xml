<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@id/softkey_template_9key_left_panel" layout="@layout/softkey_9key_side_panel_label" popup_timing="ON_TOUCH" content_description="$desc$">
            <action type="PRESS" data="$press_data$" keycode="PLAIN_TEXT" intention="$press_intention=COMMIT$" />
            <label location="@id/label" value="$press_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_9key_left_panel">
            <softkey id="@id/softkey_ellipsis_zh_left_panel" press_data="…" desc="@string/ellipsis_key_content_desc" />
            <softkey id="@id/softkey_wave_zh_left_panel" press_data="～" desc="@string/full_width_tilde_key_content_desc" />
            <softkey id="@id/softkey_question_zh_left_panel" press_data="？" desc="@string/full_width_question_mark_key_content_desc" />
            <softkey id="@id/softkey_exclamation_zh_left_panel" press_data="！" desc="@string/full_width_exclamation_key_content_desc" />
            <softkey id="@id/softkey_semicolon_zh_left_panel" press_data="；" desc="@string/full_width_semicolon_key_content_desc" />
            <softkey id="@id/softkey_colon_zh_left_panel" press_data="：" desc="@string/full_width_colon_key_content_desc" />
            <softkey id="@id/softkey_minus" press_data="-" />
            <softkey id="@id/softkey_at" press_data="\@" />
            <softkey id="@id/softkey_quote" press_data="&quot;" press_intention="DECODE" />
            <softkey id="@id/softkey_percent" press_data="%" />
            <softkey id="@id/softkey_slash" press_data="/" />
        </softkey_list>
    </softkeys>
</framework>