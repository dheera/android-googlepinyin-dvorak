<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_fragment_action_key_popup_insert_new_line" popup_timing="ON_TOUCH">
            <action type="LONG_PRESS" keycode="PLAIN_TEXT" data="\n" popup_icon="@attr/IconEnter" popup_layout="@attr/PopupBubbleActionLayout" />
        </softkey>
        <softkey id="@id/softkey_fragment_action_key_popup_previous" popup_timing="ON_TOUCH">
            <action type="LONG_PRESS" keycode="IME_ACTION" data="previous" popup_icon="@attr/IconPrevious" popup_layout="@attr/PopupBubbleActionLayout" />
        </softkey>
        <softkey id="@id/softkey_fragment_action_key_popup_next" popup_timing="ON_TOUCH">
            <action type="LONG_PRESS" keycode="IME_ACTION" data="next" popup_icon="@attr/IconNext" popup_layout="@attr/PopupBubbleActionLayout" />
        </softkey>
    </softkeys>
</framework>