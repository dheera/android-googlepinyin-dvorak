<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_non_prime_main_category_digit" layout="@layout/softkey_non_prime_main_category_icon" content_description="@string/digit_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="digit" />
            <icon location="@id/icon" value="@attr/IconMainCategoryNumber" />
        </softkey>
        <softkey id="@id/softkey_non_prime_main_category_symbol" layout="@layout/softkey_non_prime_main_category_icon" content_description="@string/symbol_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="symbol" />
            <icon location="@id/icon" value="@attr/IconMainCategorySymbol" />
        </softkey>
        <softkey id="@id/softkey_non_prime_main_category_emoji" layout="@layout/softkey_icon" content_description="@string/emoji_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="smiley" />
            <icon location="@id/icon" value="@attr/IconMainCategorySmiley" />
        </softkey>
        <softkey id="@id/softkey_non_prime_main_category_emoticon" layout="@layout/softkey_non_prime_main_category_icon" content_description="@string/emoticon_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="SWITCH_KEYBOARD" data="emoticon" />
            <icon location="@id/icon" value="@attr/IconMainCategoryEmoticon" />
        </softkey>
        <softkey id="@id/softkey_non_prime_main_category_draw" layout="@layout/softkey_non_prime_main_category_icon" content_description="@string/emoticon_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="OPEN_EXTENSION" data="com.google.android.apps.inputmethod.libs.emojihandwriting.IEmojiHandwritingExtension" />
            <icon location="@id/icon" value="@attr/IconMainCategoryDraw" />
        </softkey>
        <include href="@xml/softkeys_non_prime_category_localized" />
    </softkeys>
</framework>