<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@id/softkey_template_symbol_number" layout="@layout/softkey_label_medium" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
            <action type="PRESS" keycode="$param_keycode$" data="$param_data$" intention="COMMIT" popup_label="$param_data$" />
            <action type="LONG_PRESS" data="$long_press_data$" keycode="PLAIN_TEXT" intention="COMMIT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_template id="@id/softkey_template_light_symbol" layout="@layout/softkey_label_medium_auto_centered" multi_touch="false" popup_timing="@string/qwerty_popup_timing" content_description="$desc$">
            <action type="PRESS" keycode="SHORT_TEXT" data="$param_data$" intention="COMMIT" popup_label="$param_data$" />
            <action type="LONG_PRESS" data="$long_press_data$" keycode="SHORT_TEXT" intention="COMMIT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_template id="@id/softkey_template_light_symbol_auto_sized" layout="@layout/softkey_label_medium_auto_centered" multi_touch="false" popup_timing="@string/qwerty_popup_timing" content_description="$desc$">
            <action type="PRESS" keycode="SHORT_TEXT" data="$param_data$" intention="COMMIT" popup_label="$param_data$" popup_layout="@attr/PopupBubbleAutoSizedLayout" />
            <action type="LONG_PRESS" data="$long_press_data$" keycode="SHORT_TEXT" intention="COMMIT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
    </softkeys>
</framework>